




mixin git_btn(link)
  .header__btn.btn.btn-primary-outline
    div(class=link)
      a(href="https://github.com/eignhpants") eignhpants


mixin history(history)
  .history
    h1.history__company= history.company
    h2.history__title= history.title
    h4.history__date= history.date
    p.history__description= history.description

//- content mixins
mixin bullet(header)
  .bullet.col-md-4
    h2= header
    block


mixin button-bar()
  .buttons
    button.btn-group
      button.btn.btn-default left
      button.btn.btn-default middle
      button.btn.btn-default right